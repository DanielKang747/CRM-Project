import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.io.StreamTokenizer;
import java.util.Scanner;

public class Filereader {

	public static void main (String[] args) throws IOException {
		
	    FileReader in = new FileReader("backup.csv");
	    BufferedReader br = new BufferedReader(in);
	    
	    Scanner fileScanner = new Scanner(br);
	    fileScanner.nextLine();
	    
	    while (fileScanner.hasNextLine()) {
	      String line = fileScanner.nextLine();

	      Scanner lineScanner = new Scanner(line);
	      
	      lineScanner.useDelimiter(",");
	      
	      while (lineScanner.hasNext()) {
	        
    			String custID = lineScanner.next();
    			String lastName = lineScanner.next();
    			String firstName = lineScanner.next();
    			String phoneNum = lineScanner.next();
    			System.out.println("Birth Date : " + lineScanner.next());
    			System.out.println("Email : " + lineScanner.next());
    			System.out.println("PriCustID : " + lineScanner.next());
    			System.out.println("Memo : " + lineScanner.next());
    			System.out.println("PostalCD : " + lineScanner.next());
    			System.out.println("AddrStNm : " + lineScanner.next());
    			System.out.println("AddrPvNm : " + lineScanner.next());
    			System.out.println("ChgDateTime : " + lineScanner.next());
    			
    			
	      }
	      lineScanner.close();
	      
	      System.out.println("\n");
	    }
	    fileScanner.close();
 	}
}
