						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.expDateField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.policyNoField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.premiumPanelForAuto);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.textDateTimeField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.callDateTimeField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.mailDateTimeField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.completeDateField);	
