						GUIForMemberDialog.jDialogForCommercial.setLocationRelativeTo(GUIForMemberDialog.jFrameForRegister);
						GUIForMemberDialog.jDialogForCommercial.setResizable(false);
						break;
				}
			case DatabaseConstant.EDIT_MODE:
				switch (newInfo) {
					case DatabaseConstant.CUSTOMER_MODE:
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.idLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.lastNameLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.firstNameLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.cellLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.birthDateLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.emailLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.priCustIDLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.addrStNumLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.addrPvNumLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.postalCdLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.memoLabel);

						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.idPanel);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.lastNameField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.firstNameField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.cellField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.birthDateTextField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.emailTextField);		
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.priCustIDTextField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.addrStrtNumTextField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.addrPvNumTextField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.postalCdField);
						GUIForMemberDialog.fieldValuePanel.add(GUIForMemberDialog.memoTextFieldArea);
						GUIForMemberDialog.memoTextField.setLineWrap(true);
						GUIForMemberDialog.memoTextField.setWrapStyleWord(true);
						
						GUIForMemberDialog.idPanel.add(GUIForMemberDialog.idField);
						GUIForMemberDialog.idField.setEnabled(false);//to disable further editing of ID when creating a new profile
						GUIForMemberDialog.idField.setText(String.valueOf(GUIForCustomerCare.idTextField.getText().trim()));
						
						try {
							Utilities.connection = DriverManager.getConnection(DatabaseConstant.URL, DatabaseConstant.USER, DatabaseConstant.PASSWORD);
							
							Utilities.statement = Utilities.connection.createStatement();
												
							Utilities.resultSet = Utilities.statement.executeQuery("SELECT * FROM CCCUSTOM where CCCUSTOM.CUST_ID = \"" + GUIForMemberDialog.idField.getText().trim() + "\"");
										
							GUIForMemberDialog.lastNameField.removeAll();
							GUIForMemberDialog.firstNameField.removeAll();
							GUIForMemberDialog.cellField.removeAll();
							GUIForMemberDialog.birthDateTextField.removeAll();
							GUIForMemberDialog.emailTextField.removeAll();
							GUIForMemberDialog.priCustIDTextField.removeAll();
							GUIForMemberDialog.memoTextField.removeAll();
							GUIForMemberDialog.postalCdField.removeAll();
							GUIForMemberDialog.addrStrtNumTextField.removeAll();
							GUIForMemberDialog.addrPvNumTextField.removeAll();
							
							while (Utilities.resultSet.next()) {
								String lastName = Utilities.resultSet.getString(2);
								String firstName = Utilities.resultSet.getString(3);
								String cellNo = Utilities.resultSet.getString(4);
								String birthDate = Utilities.resultSet.getString(5);
								String email = Utilities.resultSet.getString(6);
								String primaryID = Utilities.resultSet.getString(7);
								String memo = Utilities.resultSet.getString(8);
								String postalCD = Utilities.resultSet.getString(9);
								String addrStrt = Utilities.resultSet.getString(10);
								String addrPv = Utilities.resultSet.getString(11);

								GUIForMemberDialog.lastNameField.setText(lastName);
								GUIForMemberDialog.firstNameField.setText(firstName);
								GUIForMemberDialog.cellField.setText(cellNo);
								
								GUIForMemberDialog.birthDateTextField.setText(birthDate);
								GUIForMemberDialog.emailTextField.setText(email);	
								GUIForMemberDialog.priCustIDTextField.setText(primaryID);
								GUIForMemberDialog.memoTextField.setText(memo);
								GUIForMemberDialog.postalCdField.setText(postalCD);
