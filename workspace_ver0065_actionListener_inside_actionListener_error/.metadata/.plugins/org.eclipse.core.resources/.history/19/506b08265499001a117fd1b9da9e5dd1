						break;
					case DatabaseConstant.COMMERCIAL_MODE:
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.idLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.timeStampLabel);
						GUIForMemberDialog.fieldNamePanel.add(GUIForMemberDialog.txnTypeLabel);
