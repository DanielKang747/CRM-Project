				fieldValuePanel.add(addrPvNumTextField);
				fieldValuePanel.add(postalCdField);
				fieldValuePanel.add(memoTextFieldArea);
				memoTextField.setLineWrap(true);
				memoTextField.setWrapStyleWord(true);
				
				idPanel.add(idField);
