	String[] txnTypeStringsForAutomobile = {"NEW PLATE", "RENEWAL", "TRANSFER", "CHANGE", "CANCEL", "CLAIM", "AMEND"};
	JComboBox<String> txnTypeField = new JComboBox<String>(txnTypeStrings);
