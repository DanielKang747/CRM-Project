	        		public boolean getCheatCode() {
	        			return (mods & InputEvent.CTRL_MASK);
	        		}
				
	        		isCheatCode(KeyEvent e) {
				    if(e.getKeyCode() == KeyEvent.VK_T) {
						int mods = e.getModifiers();
				        return (mods & InputEvent.CTRL_MASK) != 0;
				    }
				    return false;
				}
		    });
		      
