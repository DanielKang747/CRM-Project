			cancelButton.addActionListener(new ActionListener()
			{
				@Override
				public void actionPerformed(ActionEvent e)
				{
					jDialog.dispose();
					jDialogForAuto.dispose();
					jDialogForTravel.dispose();
					jDialogForHome.dispose();
					jDialogForCommercial.dispose();
