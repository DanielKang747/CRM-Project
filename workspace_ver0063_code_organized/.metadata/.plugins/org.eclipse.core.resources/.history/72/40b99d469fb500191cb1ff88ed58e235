				GUIForMemberDialog.fieldValuePanel.add(addrStrtNumTextField);
				GUIForMemberDialog.fieldValuePanel.add(addrPvNumTextField);
				GUIForMemberDialog.fieldValuePanel.add(postalCdField);
				GUIForMemberDialog.fieldValuePanel.add(memoTextFieldArea);
